Neighbourhood moore 2 {
    NW [-1, 1], N [0, 1], NE [1, 1], 
     W [-1, 0], C [0, 0],  E [1, 0],
    SW [-1,-1], S [0,-1], SE [1,-1]
}

Model conway : moore {
    State live {
        |all ~= live| <= 3 and |all ~= live| >= 2
    }

    State dead {
        not (|all ~= live| <= 3 and |all ~= live| >= 2)
    }
}