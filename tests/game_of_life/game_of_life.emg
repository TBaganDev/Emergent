neighbourhood moore : 2 {
    NW [-1, 1], N [0, 1], NE [1, 1], 
     W [-1, 0], C [0, 0],  E [1, 0],
    SW [-1,-1], S [0,-1], SE [1,-1]
}

model conway : moore {
    // this state does this
    state live {
        |any of all == live| <= 3 and |any of all == live| >= 2
    }

    default state dead
}